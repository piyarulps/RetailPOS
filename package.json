{
	"name": "retailpos",
	"description": "Insight Retail POS",
	"version": "0.0.1",
	"main": "main.js",
	"scripts": {
		"ng": "ng",
		"start": "ng serve && node server",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"compile:server": "webpack --config webpack.server.config.js --progress --colors",
		"serve:ssr": "node server",
		"build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
		"build:client-and-server-bundles": "ng build --prod && ng run irsfrontend:server:production",
		"electron-tsc": "tsc main.ts && ng build --base-href ./ && electron .",
		"electron": "ng build --base-href ./ && electron .",
		"package-mac": "electron-packager . RPOS-App --overwrite --platform=darwin --arch=x64 --icon=dist/assets/icons/favicon.png.icns --prune=true --out=mac-release-builds",
		"package-win": "electron-packager . RPOS-App --overwrite --asar --platform=win32 --arch=x64 --icon=dist/assets/icons/favicon.png.ico --out=release-builds --version-string.CompanyName=NAVSOFT --version-string.FileDescription=CE --version-string.ProductName=\"RPOS App\"",
		"package-linux": "electron-packager . RPOS-App --overwrite --asar --platform=linux --arch=x64 --icon=dist/assets/icons/favicon.png.ico --prune=true --out=release-builds --executableName=irsfrontend",
		"create-installer-win": "node installers/windows/createinstaller.js",
		"create-installer-mac": "electron-installer-dmg ./mac-release-builds/Electron\\ RPOS\\ RPOSAPP-darwin-x64/Electron\\ RPOS\\ RPOSAPP.app electron-rpos-app --out=mac-release-builds --overwrite --icon=dist/assets/icons/favicon.png.icns",
		"create-installer-debian": "electron-installer-debian --src release-builds/RPOS-App-linux-x64/ --arch amd64 --config installers/linux/debian.json",
		"electron-rebuild": "sudo $(npm bin)/electron-rebuild",
		"copy-icons": "cp assets/icons/* dist/assets/icons/",
		"build-mac": "npm run package-mac && npm run create-installer-mac",
		"build-win": "npm run package-win && npm run create-installer-win",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"dist:win": "electron-builder --win",
		"dist:mac": "electron-builder --dmg",
		"dist:linux": "electron-builder --linux",
		"postinstall": "electron-builder install-app-deps",
		"postinstalldev": "electron-rebuild --force"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^7.2.15",
		"@angular/cdk": "^7.3.7",
		"@angular/common": "^7.2.15",
		"@angular/compiler": "^7.2.15",
		"@angular/core": "^7.2.15",
		"@angular/forms": "^7.2.15",
		"@angular/http": "^7.2.15",
		"@angular/material": "^7.3.7",
		"@angular/platform-browser": "^7.2.15",
		"@angular/platform-browser-dynamic": "^7.2.15",
		"@angular/platform-server": "^7.2.15",
		"@angular/router": "^7.2.15",
		"@ng-select/ng-select": "^2.20.5",
		"@nguniversal/express-engine": "^7.1.1",
		"@nguniversal/module-map-ngfactory-loader": "0.0.0",
		"angular2-hotkeys": "^2.1.5",
		"body-parser": "^1.19.0",
		"bootstrap": "^4.4.1",
		"bootstrap-scss": "^4.4.1",
		"core-js": "^2.6.11",
		"custom-electron-titlebar": "^3.2.0",
		"dat-swarm-defaults": "^1.0.2",
		"discovery-swarm": "^6.0.1",
		"elasticsearch": "^16.5.0",
		"elasticsearch-browser": "^15.5.0",
		"electron-packager": "^14.1.1",
		"electron-squirrel-startup": "^1.0.0",
		"escpos": "^2.5.2",
		"express": "^4.17.1",
		"file-system": "^2.2.2",
		"fs-extra": "^8.1.0",
		"get-port": "^5.1.1",
		"getmac": "^4.4.0",
		"graceful-fs": "^4.2.3",
		"jquery": "^3.4.1",
		"mime": "^2.4.4",
		"mongoose": "^5.9.2",
		"multer": "^1.4.2",
		"nedb": "^1.8.0",
		"ng-connection-service": "^1.0.4",
		"ngx-currency": "^2.2.1",
		"node-hid": "^1.2.0",
		"os": "^0.1.1",
		"path": "^0.12.7",
		"popper.js": "^1.16.0",
		"portfinder": "^1.0.25",
		"prebuild": "^9.1.1",
		"prebuild-install": "^5.3.3",
		"rxjs": "~6.4.0",
		"sequelize": "^5.21.5",
		"tslib": "^1.11.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.13.4",
		"@angular/cli": "^7.3.9",
		"@angular/compiler-cli": "^7.2.15",
		"@angular/language-service": "^7.2.15",
		"@types/elasticsearch": "^5.0.36",
		"@types/electron": "^1.6.10",
		"@types/mysql": "^2.15.9",
		"@types/node": "^8.10.59",
		"asar": "^2.1.0",
		"cross-var": "^1.1.0",
		"electron": "^7.1.13",
		"electron-builder": "^22.4.0",
		"electron-installer-dmg": "^3.0.0",
		"electron-installer-windows": "^2.0.0",
		"electron-rebuild": "^1.8.8",
		"electron-winstaller": "^3.0.4",
		"ngx-spec": "^2.1.4",
		"node-sass": "^4.13.1",
		"protractor": "~5.4.0",
		"ts-loader": "^5.4.5",
		"ts-node": "~7.0.0",
		"tslint": "~5.11.0",
		"typescript": "~3.2.2",
		"webpack-cli": "^3.3.11"
	},
	"build": {
		"appId": "com.rpos.apm",
		"productName": "Insight Retail POS",
		"directories": {
			"buildResources": "build",
			"output": "electron-builder-builds"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true,
			"perMachine": false,
			"installerIcon": "build/icons/256x256.ico",
			"license": "build/license.txt",
			"installerSidebar": "build/installerSidebar.bmp"
		},
		"linux": {
			"target": "AppImage",
			"executableName": "Insight Retail POS",
			"icon": "build/icons/512x512.png"
		},
		"mac": {}
	},
	"files": [
		"*.js",
		"node_modules"
	]
}
